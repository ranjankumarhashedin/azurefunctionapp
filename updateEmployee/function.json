{
  "bindings": [
    {
      "authLevel": "anonymous",
      "name": "req",
      "type": "httpTrigger",
      "direction": "in",
      "methods": [
        "get",
        "post"
      ],
      "route": "records/{id}"
    },
    {
      "name": "$return",
      "type": "http",
      "direction": "out"
    },
    {
      "type": "cosmosDB",
      "name": "employeeDocument",
      "databaseName": "DBNAME",
      "collectionName": "COLLECTIONNAME",
      "connectionStringSetting": "cf-cmp-cosmosdb_DOCUMENTDB",
      "direction": "in"
    },
    {
      "type": "cosmosDB",
      "name": "outputDocument",
      "databaseName": "DBNAME",
      "collectionName": "COLLECTIONNAME",
      "createIfNotExists": false,
      "Id" : "{id}",
      "connectionStringSetting": "cf-cmp-cosmosdb_DOCUMENTDB",
      "direction": "out"
    }
  ],
  "disabled": false
}